{
  "name": "iroots",
  "description": "A CLI to manage Trellis projects",
  "version": "0.0.0",
  "author": "Tang Rufus @TangRufus",
  "bin": {
    "iroots": "./bin/run"
  },
  "bugs": "https://github.com/itinerisltd/iroots/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "cli-ux": "^5.1.0",
    "execa": "^1.0.0",
    "fs-extra": "^7.0.1",
    "globby": "^9.0.0",
    "replace-in-file": "^3.4.3",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/tslint": "^3",
    "@types/execa": "^0.9.0",
    "@types/fs-extra": "^5.0.4",
    "@types/globby": "^8.0.0",
    "@types/node": "^10",
    "ts-node": "^7",
    "tslint": "^5",
    "typescript": "^3.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/itinerisltd/iroots",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "iroots",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "itinerisltd/iroots",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p . -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
